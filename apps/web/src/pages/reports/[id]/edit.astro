---
import DashboardLayout from '../../../layouts/DashboardLayout.astro';
import ReportEditPage from '../../../components/reports/ReportEditPage';

const { id } = Astro.params;

if (!id) {
  return Astro.redirect('/reports');
}
---

<DashboardLayout title="Edit Report">
  <ReportEditPage client:load reportId={id} />
</DashboardLayout>
